  name = Martin Mu√±oz
  email = im.mmun@gmail.com
[core]
  excludesfile = ~/.gitignore
  editor = /usr/bin/vim
[push]
  default = tracking
[alias]
  pr = "!f() { git fetch origin refs/pull/$1/head:pr/$1; } ; f"
  co = checkout
  ci = commit
  st = status
  br = branch
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  type = cat-file -t
  dump = cat-file -p
[color]
  ui = true
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = green bold
  changed = yellow bold
  untracked = red
